<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>List UserCourse</title>
</head>
<body>
    <h1>List User-Course</h1>
    <table class="table">
        <thead>
            <tr>
                <th>Username</th>
                <th>Email</th>
                <th>Course</th>
            </tr>
        </thead>
        <tbody>
            <% list.forEach(each => { %>
             <tr>
                <td><%= each.username %></td>
                <td><%= each.email %></td>
                <td><%= each.Course.name %></td>
             </tr>
            <% }) %>
        </tbody>
    </table>
    <%- include('partials/_boostraps') %>
</body>
</html>